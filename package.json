{
  "name": "clank8y",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "description": "A PR review bot for Cumulocity IoT (c8y) with GitHub Action + MCP tooling",
  "keywords": [
    "typescript",
    "clank8y",
    "cumulocity",
    "c8y",
    "github-actions",
    "pull-request",
    "review-bot",
    "esm",
    "vitest",
    "eslint"
  ],
  "license": "MIT",
  "author": "schplitt",
  "homepage": "https://github.com/schplitt/clank8y#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/schplitt/clank8y.git"
  },
  "bugs": {
    "url": "https://github.com/schplitt/clank8y/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "prerelease": "eslint && tsc --noEmit && tsdown",
    "release": "bumpp",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@actions/core": "catalog:",
    "@actions/github": "catalog:",
    "@github/copilot-sdk": "catalog:",
    "@tmcp/adapter-valibot": "catalog:",
    "@tmcp/transport-http": "catalog:",
    "@toon-format/toon": "catalog:",
    "changelogithub": "catalog:",
    "consola": "catalog:",
    "defu": "catalog:",
    "octokit": "catalog:",
    "srvx": "catalog:",
    "tinyexec": "catalog:",
    "tmcp": "catalog:",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@schplitt/eslint-config": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
